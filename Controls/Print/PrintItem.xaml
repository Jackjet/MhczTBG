<UserControl x:Class="MhczTBG.Controls.Print.PrintItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
            Width="820" Height="1050">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="15"/>
            <RowDefinition/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>
        <TextBlock Name="txtTitle" FontSize="18" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,-15" Text="1990/10/16"/>
        <Border Name="bordMain" Grid.Row="1"/>
        <TextBlock FontSize="14" Name="txtPageFoot" HorizontalAlignment="Right" VerticalAlignment="Center" Text="北京通信段" Margin="15,0" Grid.Row="2"/>
    </Grid>
</UserControl>
